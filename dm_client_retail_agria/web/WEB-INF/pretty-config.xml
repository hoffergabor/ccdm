<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.2.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.2.0 http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.2.0.xsd">


	<url-mapping id="index">
		<pattern value="/index" />
		<view-id value="/index.jsf"></view-id>
	</url-mapping>
	
	<url-mapping id="index_a">
		<pattern value="/index_a" />
		<view-id value="/index_a.jsf"></view-id>
	</url-mapping>

	<url-mapping id="news">
		<pattern value="/hirek" />
		<view-id value="/news.jsf"></view-id>
	</url-mapping>

	<url-mapping id="profile">
		<pattern value="/profile" />
		<view-id value="/profile.jsf"></view-id>
	</url-mapping>

	<url-mapping id="product">
		<pattern value="/termekek" />
		<view-id value="/products.jsf"></view-id>
	</url-mapping>

	<url-mapping id="registration">
		<pattern value="/regisztracio" />
		<view-id value="/registration.jsf"></view-id>
	</url-mapping>

	<url-mapping id="productlevel1">
		<pattern value="/termekek/kategoria/#{productsController.data.categoryId}/" />
		<view-id value="/products.jsf"></view-id>
		<action onPostback="false">#{productsController.showProducts}</action>
	</url-mapping>

	<url-mapping id="productlevel2">
		<pattern
			value="/termekek/kategoria/#{productsController.data.categoryId}/#{productsController.data.category2Id}/" />
		<view-id value="/products.jsf"></view-id>
		<action onPostback="false">#{productsController.showProductsLevel2}</action>
	</url-mapping>

	<url-mapping id="productlevel3">
		<pattern
			value="/termekek/kategoria/#{productsController.data.categoryId}/#{productsController.data.category2Id}/#{productsController.data.category3Id}/" />
		<view-id value="/products.jsf"></view-id>
		<action onPostback="false">#{productsController.showProductsLevel3}</action>
	</url-mapping>

	<url-mapping id="productlevel4">
		<pattern
			value="/termekek/kategoria/#{productsController.data.categoryId}/#{productsController.data.category2Id}/#{productsController.data.category3Id}/#{productsController.data.category4Id}/" />
			<view-id value="/products.jsf"></view-id>
		<action onPostback="false">#{productsController.showProductsLevel4}</action>
	</url-mapping>

	<url-mapping id="product3">
		<pattern value="/termekek/kereses/#{productsController.data.searchCikkcsop}/" />
		<view-id value="/products.jsf"></view-id>
		<action onPostback="false">#{productsController.searchProductByCikkcsop}</action>
	</url-mapping>

	<url-mapping id="product4">
		<pattern value="/termek/#{productController.data.productId}/" />
		<view-id value="/product.jsf"></view-id>
		<action>#{productController.showSelectedProduct}</action>
	</url-mapping>


	<url-mapping id="cart" onPostback="true">
		<pattern value="/kosar" />
		<view-id value="/cart.jsf"></view-id>
	</url-mapping>

	<url-mapping id="forgottenpassword">
		<pattern value="/elfelejtettjelszo" />
		<view-id value="/forgottenpassword.jsf"></view-id>
	</url-mapping>

	<url-mapping id="order">
		<pattern value="/rendeles" />
		<view-id value="/order.jsf"></view-id>
	</url-mapping>

	<url-mapping id="orderlist">
		<pattern value="/rendelesek" />
